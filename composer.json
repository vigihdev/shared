{
    "name": "vigihdev/shared",
    "description": "A collection of DTOs and Contracts of shared utilities for PHP.",
    "homepage": "https://github.com/vigihdev/shared",
    "keywords": [
        "dto",
        "contract",
        "performance"
    ],
    "type": "library",
    "license": "MIT",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "authors": [
        {
            "name": "Vigih Dev",
            "email": "vigihdev@gmail.com",
            "role": "developer"
        }
    ],
    "autoload": {
        "psr-4": {
            "Vigihdev\\Shared\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Vigihdev\\Shared\\Tests\\": "tests/"
        }
    },
    "config": {
        "process-timeout": 0
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://repo.thrubus.id"
        }
    ],
    "require": {
        "php": "^8.1",
        "symfony/serializer": "^6.4"
    },
    "require-dev": {
        "phpunit/phpunit": "10.5.x-dev",
        "vigihdev/serializer-bridge": "dev-main"
    },
    "scripts": {
        "test": "phpunit",
        "dev": "php -S localhost:1392 local-dev/index.php"
    }
}